{
  "dashboard": {
    "id": null,
    "title": "Workspace Qdrant MCP - System Overview",
    "tags": ["workspace-qdrant-mcp", "observability"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "system_health_status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "Unhealthy", "color": "red"},
                  "1": {"text": "Healthy", "color": "green"}
                }
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mcp_requests_total[5m])",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, mcp_request_duration_seconds_bucket)",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, mcp_request_duration_seconds_bucket)",
            "refId": "B", 
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, mcp_request_duration_seconds_bucket)",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mcp_requests_total{status=~\"error|timeout\"}[5m]) / rate(mcp_requests_total[5m])",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate (%)",
            "max": 1,
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Active Searches",
        "type": "stat",
        "targets": [
          {
            "expr": "search_operations_active",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Document Processing Queue",
        "type": "graph",
        "targets": [
          {
            "expr": "document_processing_queue_length",
            "refId": "A",
            "legendFormat": "Queue Length"
          },
          {
            "expr": "rate(documents_processed_total[5m])",
            "refId": "B",
            "legendFormat": "Processing Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_memory_usage_bytes / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Memory MB"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 8,
        "title": "Qdrant Collections",
        "type": "stat",
        "targets": [
          {
            "expr": "qdrant_collections_count",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 9,
        "title": "File Watchers Status",
        "type": "graph",
        "targets": [
          {
            "expr": "file_watchers_active",
            "refId": "A",
            "legendFormat": "Active Watchers"
          },
          {
            "expr": "file_watchers_errors",
            "refId": "B",
            "legendFormat": "Error Watchers"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40}
      },
      {
        "id": 10,
        "title": "Health Check Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "health_check_duration_seconds",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 48}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": []
  }
}