{
  "dashboard": {
    "id": null,
    "title": "Workspace Qdrant MCP - System Metrics",
    "tags": ["workspace-qdrant-mcp", "system", "infrastructure"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage by Core",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by (cpu) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU {{cpu}}"
          }
        ],
        "yAxes": [
          {
            "label": "Percent",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fill": 2,
        "linewidth": 1
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes",
            "legendFormat": "Used Memory"
          },
          {
            "expr": "node_memory_MemAvailable_bytes",
            "legendFormat": "Available Memory"
          },
          {
            "expr": "node_memory_Cached_bytes",
            "legendFormat": "Cached Memory"
          },
          {
            "expr": "node_memory_Buffers_bytes",
            "legendFormat": "Buffer Memory"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "stack": true
      },
      {
        "id": 3,
        "title": "Disk I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_disk_read_bytes_total[5m])",
            "legendFormat": "Read {{device}}"
          },
          {
            "expr": "rate(node_disk_written_bytes_total[5m]) * -1",
            "legendFormat": "Write {{device}}"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total{device!=\"lo\"}[5m])",
            "legendFormat": "Receive {{device}}"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total{device!=\"lo\"}[5m]) * -1",
            "legendFormat": "Transmit {{device}}"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Filesystem Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (node_filesystem_avail_bytes{fstype!=\"tmpfs\"} / node_filesystem_size_bytes{fstype!=\"tmpfs\"} * 100)",
            "legendFormat": "{{mountpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Percent",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [80],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "model": "A",
                "params": ["5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "last"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "Disk Usage Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 6,
        "title": "Load Average",
        "type": "graph",
        "targets": [
          {
            "expr": "node_load1",
            "legendFormat": "1m load"
          },
          {
            "expr": "node_load5",
            "legendFormat": "5m load"
          },
          {
            "expr": "node_load15",
            "legendFormat": "15m load"
          }
        ],
        "yAxes": [
          {
            "label": "Load",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Context Switches & Interrupts",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_context_switches_total[5m])",
            "legendFormat": "Context Switches"
          },
          {
            "expr": "rate(node_intr_total[5m])",
            "legendFormat": "Interrupts"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "System Uptime",
        "type": "singlestat",
        "targets": [
          {
            "expr": "node_time_seconds - node_boot_time_seconds",
            "legendFormat": "Uptime"
          }
        ],
        "format": "s",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 32
        },
        "colorBackground": false,
        "colorValue": false
      },
      {
        "id": 9,
        "title": "Number of Processes",
        "type": "singlestat",
        "targets": [
          {
            "expr": "node_procs_running",
            "legendFormat": "Running"
          }
        ],
        "format": "short",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Open File Descriptors",
        "type": "singlestat",
        "targets": [
          {
            "expr": "node_filefd_allocated",
            "legendFormat": "Open FDs"
          }
        ],
        "format": "short",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "Network Errors",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(node_network_receive_errs_total[5m]) + rate(node_network_transmit_errs_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "format": "short",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 32
        },
        "colorBackground": true,
        "colorValue": false,
        "thresholds": "0,1",
        "colors": [
          "#73BF69",
          "#FADE2A",
          "#F2495C"
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(node_cpu_seconds_total, instance)",
          "refresh": 1,
          "includeAll": false,
          "multi": false
        }
      ]
    }
  }
}