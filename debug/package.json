{
  "name": "workspace-qdrant-mcp-debug",
  "version": "1.0.0",
  "description": "MCP Inspector debugging tools for workspace-qdrant-mcp",
  "scripts": {
    "debug-stdio": "npx @modelcontextprotocol/inspector --config mcp-inspector-config.json --server workspace-qdrant-stdio",
    "debug-http": "npx @modelcontextprotocol/inspector --config mcp-inspector-config.json --server workspace-qdrant-http",
    "debug-sse": "npx @modelcontextprotocol/inspector --config mcp-inspector-config.json --server workspace-qdrant-sse",
    "debug-ui": "npx @modelcontextprotocol/inspector",
    "debug-cli-stdio": "npx @modelcontextprotocol/inspector --cli --config mcp-inspector-config.json --server workspace-qdrant-stdio",
    "debug-cli-http": "npx @modelcontextprotocol/inspector --cli --config mcp-inspector-config.json --server workspace-qdrant-http",
    "list-tools": "npx @modelcontextprotocol/inspector --cli --config mcp-inspector-config.json --server workspace-qdrant-stdio --method tools/list",
    "list-resources": "npx @modelcontextprotocol/inspector --cli --config mcp-inspector-config.json --server workspace-qdrant-stdio --method resources/list",
    "test-connection": "npx @modelcontextprotocol/inspector --cli --config mcp-inspector-config.json --server workspace-qdrant-stdio --method tools/list"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "latest"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}